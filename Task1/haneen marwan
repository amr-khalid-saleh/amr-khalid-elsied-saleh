def selection_sort(arr):
    for i in range(0, len(arr) - 1):  
        k = i  
        for j in range(i + 1, len(arr)):
            if arr[j] < arr[k]: 
                k = j
        arr[i], arr[k] = arr[k], arr[i]  


arr = [3, 7, 6, 1,25, 11, 2, 5, 10, 8] 
selection_sort(arr)  
print(arr)  

